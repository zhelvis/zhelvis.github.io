{
  "name": "gatsby-i18n",
  "private": true,
  "description": "Gatsby example site using MDX and dependency-free i18n",
  "author": "Vladimir Zhelvis",
  "version": "0.1.0",
  "license": "MIT",
  "scripts": {
    "build": "gatsby build",
    "develop": "gatsby develop",
    "start": "npm run develop",
    "prettier": "prettier --write ./\\*\\*/*.{js,css,mdx,json}"
  },
  "dependencies": {
    "@mdx-js/mdx": "^1.5.1",
    "@mdx-js/react": "^1.5.1",
    "@reach/router": "^1.2.1",
    "gatsby": "^2.17.10",
    "gatsby-plugin-mdx": "^1.0.55",
    "gatsby-plugin-react-helmet": "^3.1.14",
    "gatsby-source-filesystem": "^2.1.35",
    "gatsby-transformer-json": "^2.2.16",
    "react": "^16.11.0",
    "react-dom": "^16.11.0",
    "react-helmet": "^5.2.1"
  },
  "devDependencies": {
    "husky": "^3.0.9",
    "lint-staged": "^9.4.2",
    "prettier": "^1.19.1"
  },
  "husky": {
    "hooks": {
      "pre-commit": "lint-staged"
    }
  },
  "lint-staged": {
    "*.{js,css,json,md}": [
      "prettier --write",
      "git add"
    ]
  }
}
